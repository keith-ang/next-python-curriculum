                    {
                        "type": "Text",
                        "content": ","
                    },
                    {
                        "type": "Element",
                        "tagName": "em",
                        "attributes": {},
                        "children": [
                            {
                                "type": "Text",
                                "content": "beautifoulSoup4"
                            }
                        ]
                    },
                    {
                        "type": "Text",
                        "content": "and a"
                    },
